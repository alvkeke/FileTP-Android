<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:gravity="center_vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/process_icon"
        android:layout_marginStart="@dimen/distance_margin_file_icon"
        android:layout_marginTop="@dimen/distance_margin_file_icon"
        android:layout_marginBottom="@dimen/distance_margin_file_icon"
        android:layout_marginEnd="@dimen/distance_margin_file_icon"
        android:layout_width="36dp"
        android:layout_height="36dp"/>

    <TextView
        android:id="@+id/process_name"
        android:text="@string/app_name"
        android:textSize="16sp"
        android:textColor="@color/color_font"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</LinearLayout>